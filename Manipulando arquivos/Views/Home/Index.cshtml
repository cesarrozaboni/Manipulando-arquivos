@{
    ViewBag.Title = "Home Page";
}



@Html.ActionLink("Baixar arquivo zip", "DownloadFileZip", "Home")

<br />

@Html.ActionLink("Baixar arquivo file stream", "DownloadOneFile", "Home")

<br />

@Html.ActionLink("Baixar arquivo Particionado", "DownloadParticionado", "Home")

<br />

@Html.ActionLink("Baixar arquivo por file result", "DownloadFileResult", "Home")

<br />

<h2>UploadFile</h2>
@using (Html.BeginForm("UploadFile", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div>
        @Html.TextBox("file", "", new { type = "file" }) <br />
        <input type="submit" value="Upload" />
        @ViewBag.Message
    </div>
}

<h2>Upload Multiple File</h2>
@using (Html.BeginForm("UploadMultipleFile", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div>
        @Html.TextBox("file", "", new { type = "file", multiple="multiple" }) <br />
        <input type="submit" value="Upload" />
        @ViewBag.Message
    </div>
}